datasource DS {
  // optionally set multiple providers
  provider = ["sqlite", "postgresql"]
  url = "***"
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model Room {
  id      Int      @id @default(autoincrement())
  name    String   @unique
  ownerId String?
  humanQM Boolean? @default(false)
}
